{
    "manifest_version": 3,
    "name": "Pomodoro Extension",
    "version": "1.0.0",
    "description": "Pomodoro - Simple Extension for Example",
    "icons": {
        "16": "images/icon.png",
        "48": "images/icon.png",
        "128": "images/icon.png"
    },
    "action": {
        "default_icon": {
            "16": "images/icon.png",
            "24": "images/icon.png",
            "32": "images/icon.png"
        },
        "default_title": "Pomodoro Extension for Actions",
        "default_popup": "popup/popup.html"
    },
    "options_page": "options/options.html",
    "permissions": ["storage", "alarms", "notifications"],
    "background": {
        "service_worker": "background.js"
    }
}